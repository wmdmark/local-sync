    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.2.1",
    "cpx": "^1.5.0",
    "dir-compare": "^2.4.0",
    "figlet": "^1.5.0",
    "inquirer": "^7.3.3",
    "path": "^0.12.7",
    "semver": "^7.3.4",
    "sort-package-json": "^1.48.0"
  }
}
