    "tslib": "^2.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "clear": "^0.1.0",
    "commander": "^6.2.1",
    "cpx": "^1.5.0",
    "figlet": "^1.5.0",
    "path": "^0.12.7",
    "semver": "^7.3.4"
  }
}
